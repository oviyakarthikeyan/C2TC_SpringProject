<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">
    
    <bean id="college" class="com.tnsif.college.College"></bean>

    <bean id="student2" class="com.tnsif.college.Student">
        <property name="sid" value="101"></property>
        <property name="sname" value="Oviya"></property>
        <property name="course" value="JAVA"></property>
        <property name="c1" ref="college"></property>
    </bean>

    <bean id="student1" class="com.tnsif.college.Student">
        <property name="sid" value="102"></property>
        <property name="sname" value="Leelaa"></property>
        <property name="course" value="Python"></property>
        <property name="c1" ref="college"></property>
    </bean>
    
    <bean id="student3" class="com.tnsif.college.Student">
        <constructor-arg value="105"/>
        <constructor-arg value="Ashwini"/>
        <constructor-arg value="AI"/>
    </bean>

</beans>